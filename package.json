{
  "name": "ninsho-utils",
  "version": "0.0.6",
  "description": "A general-purpose package used on ninsho.",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/cjs/index.d.ts",
  "scripts": {
    "lint": "eslint \"src/**/*.ts\" \"src/*.ts\"",
    "prebuild": "rimraf dist",
    "test": "jest --config ./jest.config.json --runInBand --coverage",
    "build-t": "tsc",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build": "npm run build:cjs && npm run build:esm",
    "command": "npx ts-node-dev --respawn src/bin/index.ts"
  },
  "files": [
    "package.json",
    "README.md",
    "LICENSE",
    "dist"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ninsho/ninsho-utils/issues"
  },
  "homepage": "https://github.com/ninsho/ninsho-utils/",
  "dependencies": {
    "ninsho-base": "^0.0.7"
  },
  "devDependencies": {
    "@types/jest": "^29.5.1",
    "@types/node": "^20.2.3",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "eslint": "^7.32.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.5"
  },
  "sideEffects": false
}
